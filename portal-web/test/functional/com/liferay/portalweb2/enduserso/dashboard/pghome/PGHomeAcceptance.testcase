<definition>
	<set-up>
		<execute macro="SOUser#firstLoginPG" />
	</set-up>

	<var name="distributionScope" value="General" />
	<var name="entryContent" value="Announcements Entry Content" />
	<var name="entryContentEdit" value="Announcements Entry Content Edit" />
	<var name="entryTitle" value="Announcements Entry Title" />
	<var name="entryTitleEdit" value="Announcements Entry Title Edit" />
	<var name="entryURL" value="http://www.liferay.com" />
	<var name="eventTitle" value="Calendar Event Title" />
	<var name="pageName" value="Calendar Page" />
	<var name="portletName" value="Calendar" />
	<var name="userName" value="Joe Bloggs" />


	<command name="AddWhatsHappeningEntryViewableByEveryone">
		<var name="whatsHappeningEntryAuthorFirstName" value="Joe" />
		<var name="whatsHappeningEntryAuthorLastName" value="Bloggs" />
		<var name="whatsHappeningEntryAuthorScreenName" value="joebloggs" />
		<var name="whatsHappeningEntryContent" value="Microblogs Post" />
		<var name="whatsHappeningEntryCharacterCountdown" value="135" />
		<var name="whatsHappeningEntryViewableBy" value="Everyone" />
		<var name="userEmailAddress" value="socialoffice01@liferay.com" />
		<var name="userFirstName" value="Social01" />
		<var name="userMiddleName" value="Office01" />
		<var name="userLastName" value="User01" />
		<var name="userScreenName" value="socialoffice01" />

		<execute macro="SOUser#addUserWithSORole">
			<var name="adminEmailAddress" value="test@liferay.com" />
			<var name="password" value="test" />
			<var name="userEmailAddress" value="socialoffice01@liferay.com" />
			<var name="userFirstName" value="Social01" />
			<var name="userMiddleName" value="Office01" />
			<var name="userLastName" value="User01" />
			<var name="userScreenName" value="socialoffice01" />
		</execute>

		<execute macro="SOHome#addWhatsHappeningEntry">
			<var name="whatsHappeningEntryContent" value="${whatsHappeningEntryContent}" />
			<var name="whatsHappeningEntryCharacterCountdown" value="${whatsHappeningEntryCharacterCountdown}" />
			<var name="whatsHappeningEntryViewableBy" value="${whatsHappeningEntryViewableBy}" />
		</execute>

		<execute macro="SOMicroblogs#viewOwnMicroblogsEntry">
			<var name="microblogsEntryAuthorFirstName" value="${whatsHappeningEntryAuthorFirstName}" />
			<var name="microblogsEntryAuthorLastName" value="${whatsHappeningEntryAuthorLastName}" />
			<var name="microblogsEntryContent" value="${whatsHappeningEntryContent}" />
		</execute>

		<execute macro="SOUser#logoutPG" />

		<execute macro="SOUser#loginPG">
			<var name="userEmailAddress" value="${userEmailAddress}" />
			<var name="password" value="test" />
		</execute>

		<execute macro="SOMicroblogs#viewMicroblogsEntryViewableByEveryone">
			<var name="microblogsEntryAuthorFirstName" value="${whatsHappeningEntryAuthorFirstName}" />
			<var name="microblogsEntryAuthorLastName" value="${whatsHappeningEntryAuthorLastName}" />
			<var name="microblogsEntryAuthorScreenName" value="${whatsHappeningEntryAuthorScreenName}" />
			<var name="microblogsEntryContent" value="${whatsHappeningEntryContent}" />
		</execute>
	</command>

	<command name="AddSOAnnouncementsEntry">
		<execute macro="SOUserBar#dashboardPG">
			<var name="userName" value="${userName}" />
		</execute>

		<execute macro="SOAnnouncementsEntry#addPG">
			<var name="distributionScope" value="${distributionScope}" />
			<var name="entryContent" value="${entryContent}" />
			<var name="entryTitle" value="${entryTitle}" />
			<var name="entryURL" value="${entryURL}" />
		</execute>

		<execute macro="SOUserBar#dashboardPG">
			<var name="userName" value="${userName}" />
		</execute>

		<execute macro="SOAnnouncementsEntry#viewPG">
			<var name="distributionScope" value="${distributionScope}" />
			<var name="entryContent" value="${entryContent}" />
			<var name="entryTitle" value="${entryTitle}" />
			<var name="entryURL" value="${entryURL}" />
		</execute>
	</command>

	<command name="DeleteSOAnnouncementsEntry">
		<execute macro="SOUserBar#dashboardPG">
			<var name="userName" value="${userName}" />
		</execute>

		<execute macro="SOAnnouncementsEntry#addPG">
			<var name="distributionScope" value="${distributionScope}" />
			<var name="entryContent" value="${entryContent}" />
			<var name="entryTitle" value="${entryTitle}" />
			<var name="entryURL" value="${entryURL}" />
		</execute>

		<execute macro="SOUserBar#dashboardPG">
			<var name="userName" value="${userName}" />
		</execute>

		<execute macro="SOAnnouncementsEntry#deletePG" />
	</command>

	<command name="EditSOAnnouncementsEntry">
		<execute macro="SOUserBar#dashboardPG">
			<var name="userName" value="${userName}" />
		</execute>

		<execute macro="SOAnnouncementsEntry#addPG">
			<var name="distributionScope" value="${distributionScope}" />
			<var name="entryContent" value="${entryContent}" />
			<var name="entryTitle" value="${entryTitle}" />
			<var name="entryURL" value="${entryURL}" />
		</execute>

		<execute macro="SOUserBar#dashboardPG">
			<var name="userName" value="${userName}" />
		</execute>

		<execute macro="SOAnnouncementsEntry#editPG">
			<var name="distributionScope" value="${distributionScope}" />
			<var name="entryContentEdit" value="${entryContentEdit}" />
			<var name="entryTitleEdit" value="${entryTitleEdit}" />
			<var name="entryURL" value="${entryURL}" />
		</execute>

		<execute macro="SOUserBar#dashboardPG">
			<var name="userName" value="${userName}" />
		</execute>

		<execute macro="SOAnnouncementsEntry#viewPG">
			<var name="distributionScope" value="${distributionScope}" />
			<var name="entryContent" value="${entryContentEdit}" />
			<var name="entryTitle" value="${entryTitleEdit}" />
			<var name="entryURL" value="${entryURL}" />
		</execute>
	</command>

	<command name="ViewEventEventsDisplay">
		<execute macro="SOUserBar#dashboardPG">
			<var name="userName" value="${userName}" />
		</execute>

		<execute action="SOUserBar#click" locator-key1="TOGGLE_DOCKBAR" />
		<execute macro="SOHome#addPage">
			<var name="pageName" value="${pageName}" />
		</execute>

		<execute action="SOUserBar#click" locator-key1="TOGGLE_DOCKBAR" />
		<execute macro="SOHome#addPortlet">
			<var name="pageName" value="${pageName}" />
		</execute>

		<execute macro="Event#addPG">
			<var name="eventTitle" value="${eventTitle}" />
		</execute>

		<execute macro="SOHome#viewTodaysEvent">
			<var name="eventTitle" value="${eventTitle}" />
			<var name="userName" value="${userName}" />
		</execute>
	</command>

	<tear-down>
		<execute macro="SOUser#firstLoginPG" />

		<execute macro="SOMicroblogs#tearDownMicroblogsEntry" />

		<execute macro="SOAnnouncementsEntry#tearDownPG" />

		<execute macro="SOUser#tearDownCP" />

		<execute macro="SOCalendar#tearDownCP" />

		<execute macro="SOHome#tearDownPG" />
	</tear-down>
</definition>