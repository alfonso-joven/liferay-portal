<definition>
	<command name="assertTextEquals">
		<case locator-key1="INFO_MESSAGE">
			<execute function="AssertTextEquals#assertText" value1="You do not have any microblog entries." />
		</case>
		<case locator-key1="MICROBLOGS_ENTRY_COMMENT_LINK">
			<execute function="AssertTextEquals#assertText" value1="Comment" />
		</case>

		<case locator-key1="MICROBLOGS_ENTRY_EDIT_LINK">
			<execute function="AssertTextEquals#assertText" value1="Edit" />
		</case>

		<case locator-key1="MICROBLOGS_ENTRY_DELETE_LINK">
			<execute function="AssertTextEquals#assertText" value1="Delete" />
		</case>

		<case locator-key1="MICROBLOGS_ENTRY_REPOST_LINK">
			<execute function="AssertTextEquals#assertText" value1="Repost" />
		</case>
	</command>

	<command name="type">
		<case locator-key1="MICROBLOGS_CONTENT_INPUT_FIELD">
			<execute function="Type#sendKeys" />
		</case>
	</command>

	<command name="click">
		<case locator-key1="MICROBLOGS_ENTRY_DELETE_LINK">
			<execute function="Click#makeVisibleClickAt" />
		</case>
	</command>

	<command name="isElementPresent">
		<case locator-key1="MICROBLOGS_ENTRY_DELETE_LINK">
			<execute function="IsElementPresent#pauseIsElementPresent" />
		</case>
	</command>
</definition>