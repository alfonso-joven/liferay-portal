<definition>
	<command name="assertTextEquals">
		<case locator-key1="CONFIGURATION_SAVE_LOCATION">
			<execute function="AssertTextEquals#assertPartialText" />
		</case>
		<case locator-key1="CONFIGURATION_SAVE_MESSAGE">
			<execute function="AssertTextEquals#assertPartialText" />
		</case>
		<case locator-key1="SUCCESS_MESSAGE">
			<execute function="AssertTextEquals#assertText" />
		</case>
	</command>

	<command name="assertClick">
		<case locator-key1="GO_TO_MY_PORTAL_BUTTON">
			<execute function="AssertClick#assertValueClickAtAndWait" />
		</case>
	</command>
</definition>