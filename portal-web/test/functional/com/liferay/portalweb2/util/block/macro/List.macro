<definition>
	<command name="addCP">
		<execute macro="Page#cpGoto">
			<var name="portletName" value="Dynamic Data Lists" />
		</execute>

		<execute action="CPDynamicdatalists#click" locator-key1="TOOLBAR_ADD" />

		<execute action="CPDynamicdatalistsAddlist#type" locator-key1="NAME_FIELD" value1="${listName}" />
		<execute action="CPDynamicdatalistsAddlist#type" locator-key1="DESCRIPTION_FIELD" value1="${listDescription}" />
		<execute action="CPDynamicdatalistsAddlist#click" locator-key1="DATA_DEFINITION_SELECT_LINK" />

		<execute action="CPDynamicdatalistsAddlistDatadefinitions#selectFrame" locator-key1="DATA_DEFINITIONS_IFRAME" />
		<execute action="CPDynamicdatalistsAddlistDatadefinitions#type" locator-key1="BASIC_SEARCH_FIELD" value1="${dataDefinitionName}" />
		<execute action="CPDynamicdatalistsAddlistDatadefinitions#click" locator-key1="BASIC_SEARCH_BUTTON" />
		<execute action="CPDynamicdatalistsAddlistDatadefinitions#click" locator-key1="DATA_DEFINITION_TABLE_NAME_1" value1="${dataDefinitionName}" />

		<execute action="CPDynamicdatalists#selectFrameTop" />

		<execute action="CPDynamicdatalistsAddlist#assertTextEquals" locator-key1="DATA_DEFINITION_NAME" value1="${dataDefinitionName}" />
		<execute action="CPDynamicdatalistsAddlist#click" locator-key1="SAVE_BUTTON" />

		<execute action="CPDynamicdatalists#assertTextEquals" locator-key1="SUCCESS_MESSAGE" />
	</command>

	<command name="addPG">
		<execute macro="Page#pgGoto">
			<var name="portletName" value="${pageName}" />
		</execute>

		<execute action="PGDDLDisplay#click" locator-key1="TOOLBAR_ADD_LIST" />

		<execute action="PGDDLDisplayAddlist#type" locator-key1="NAME_FIELD" value1="${listName}" />
		<execute action="PGDDLDisplayAddlist#type" locator-key1="DESCRIPTION_FIELD" value1="${listDescription}" />
		<execute action="PGDDLDisplayAddlist#click" locator-key1="DATA_DEFINITION_SELECT_LINK" />

		<execute action="PGDDLDisplayAddlistDatadefinitions#selectFrame" locator-key1="DATA_DEFINITIONS_IFRAME" />
		<execute action="PGDDLDisplayAddlistDatadefinitions#type" locator-key1="BASIC_SEARCH_FIELD" value1="${dataDefinitionName}" />
		<execute action="PGDDLDisplayAddlistDatadefinitions#click" locator-key1="BASIC_SEARCH_BUTTON" />
		<execute action="PGDDLDisplayAddlistDatadefinitions#click" locator-key1="DATA_DEFINITION_TABLE_NAME_1" value1="${dataDefinitionName}" />

		<execute action="PGDDLDisplayAddlist#selectFrameTop" />
		<execute action="PGDDLDisplayAddlist#assertTextEquals" locator-key1="DATA_DEFINITION_NAME" value1="${dataDefinitionName}" />
		<execute action="PGDDLDisplayAddlist#click" locator-key1="SAVE_BUTTON" />
	</command>

	<command name="deleteCP">
		<execute macro="Page#cpGoto">
			<var name="portletName" value="Dynamic Data Lists" />
		</execute>

		<execute action="CPDynamicdatalists#type" locator-key1="BASIC_SEARCH_FIELD" value1="${listName}" />
		<execute action="CPDynamicdatalists#click" locator-key1="BASIC_SEARCH_BUTTON" />
		<execute action="CPDynamicdatalists#assertTextEquals" locator-key1="LIST_TABLE_NAME_1" value1="${listName}" />
		<execute action="CPDynamicdatalists#click" locator-key1="LIST_TABLE_ACTIONS_1" />
		<execute action="CPDynamicdatalists#click" locator-key1="LIST_MENULIST_DELETE" />
		<execute action="CPDynamicdatalists#confirm" locator-key1="CONFIRMATION" value1="Are you sure you want to delete this?" />
		<execute action="CPDynamicdatalists#assertTextEquals" locator-key1="SUCCESS_MESSAGE" />
		<execute action="CPDynamicdatalists#assertTextNotPresent" value1="${listName}" />
	</command>

	<command name="deletePG">
		<execute macro="Page#pgGoto">
			<var name="portletName" value="${pageName}" />
		</execute>

		<execute action="PGDDLDisplay#click" locator-key1="TOOLBAR_SELECT_LIST" />

		<execute action="PGDDLDisplayConfiguration#selectFrame" locator-key1="CONFIGURATION_IFRAME" />
		<execute action="PGDDLDisplayConfiguration#type" locator-key1="BASIC_SEARCH_FIELD" value1="${listName}" />
		<execute action="PGDDLDisplayConfiguration#click" locator-key1="BASIC_SEARCH_BUTTON" />
		<execute action="PGDDLDisplayConfiguration#assertTextEquals" locator-key1="LIST_TABLE_NAME_1" value1="${listName}" />
		<execute action="PGDDLDisplayConfiguration#click" locator-key1="LIST_TABLE_ACTIONS_1" />
		<execute action="PGDDLDisplayConfiguration#click" locator-key1="LIST_MENULIST_DELETE" />
		<execute action="PGDDLDisplayConfiguration#confirm" locator-key1="CONFIRMATION" value1="Are you sure you want to delete this?" />
		<execute action="PGDDLDisplayConfiguration#assertTextNotPresent" value1="${listNameNew}" />

		<execute action="PGDDLDisplay#selectFrameTop" />
	</command>

	<command name="editCP">
		<execute macro="Page#cpGoto">
			<var name="portletName" value="Dynamic Data Lists" />
		</execute>

		<execute action="CPDynamicdatalists#type" locator-key1="BASIC_SEARCH_FIELD" value1="${listName}" />
		<execute action="CPDynamicdatalists#click" locator-key1="BASIC_SEARCH_BUTTON" />
		<execute action="CPDynamicdatalists#assertTextEquals" locator-key1="LIST_TABLE_NAME_1" value1="${listName}" />
		<execute action="CPDynamicdatalists#click" locator-key1="LIST_TABLE_ACTIONS_1" />
		<execute action="CPDynamicdatalists#click" locator-key1="LIST_MENULIST_EDIT" />

		<execute action="CPDynamicdatalistsEditlist#type" locator-key1="NAME_FIELD" value1="${listNameNew}" />
		<execute action="CPDynamicdatalistsEditlist#type" locator-key1="DESCRIPTION_FIELD" value1="${listDescriptionNew}" />
		<execute action="CPDynamicdatalistsEditlist#click" locator-key1="SAVE_BUTTON" />

		<execute action="CPDynamicdatalists#assertTextEquals" locator-key1="SUCCESS_MESSAGE" />
	</command>

	<command name="editPG">
		<execute macro="Page#pgGoto">
			<var name="portletName" value="${pageName}" />
		</execute>

		<execute action="PGDDLDisplay#click" locator-key1="TOOLBAR_SELECT_LIST" />

		<execute action="PGDDLDisplayConfiguration#selectFrame" locator-key1="CONFIGURATION_IFRAME" />
		<execute action="PGDDLDisplayConfiguration#type" locator-key1="BASIC_SEARCH_FIELD" value1="${listName}" />
		<execute action="PGDDLDisplayConfiguration#click" locator-key1="BASIC_SEARCH_BUTTON" />
		<execute action="PGDDLDisplayConfiguration#assertTextEquals" locator-key1="LIST_TABLE_NAME_1" value1="${listName}" />
		<execute action="PGDDLDisplayConfiguration#click" locator-key1="LIST_TABLE_ACTIONS_1" />
		<execute action="PGDDLDisplayConfiguration#click" locator-key1="LIST_MENULIST_EDIT" />

		<execute action="PGDDLDisplayConfigurationEditlist#type" locator-key1="NAME_FIELD" value1="${listNameNew}" />
		<execute action="PGDDLDisplayConfigurationEditlist#type" locator-key1="DESCRIPTION_FIELD" value1="${listDescriptionNew}" />
		<execute action="PGDDLDisplayConfigurationEditlist#click" locator-key1="SAVE_BUTTON" />

		<execute action="PGDDLDisplayConfiguration#assertTextEquals" locator-key1="INFO_MESSAGE" value1="Displaying List: ${listNameNew}" />
		<execute action="PGDDLDisplayConfiguration#assertTextEquals" locator-key1="LIST_TABLE_NAME_1" value1="${listNameNew}" />
		<execute action="PGDDLDisplayConfiguration#assertTextEquals" locator-key1="LIST_TABLE_DESCRIPTION_1" value1="${listDescriptionNew}" />

		<execute action="PGDDLDisplay#selectFrameTop" />
	</command>

	<command name="selectPG">
		<execute macro="Page#pgGoto">
			<var name="portletName" value="${pageName}" />
		</execute>

		<execute action="PGDDLDisplay#click" locator-key1="TOOLBAR_SELECT_LIST" />

		<execute action="PGDDLDisplayConfiguration#selectFrame" locator-key1="CONFIGURATION_IFRAME" />
		<execute action="PGDDLDisplayConfiguration#type" locator-key1="BASIC_SEARCH_FIELD" value1="${listName}" />
		<execute action="PGDDLDisplayConfiguration#click" locator-key1="BASIC_SEARCH_BUTTON" />
		<execute action="PGDDLDisplayConfiguration#click" locator-key1="LIST_TABLE_NAME_1" value1="${listName}" />
		<execute action="PGDDLDisplayConfiguration#assertTextEquals" locator-key1="INFO_MESSAGE" value1="Displaying List: ${listName} (Modified)" />
		<execute action="PGDDLDisplayConfiguration#click" locator-key1="SAVE_BUTTON" />
		<execute action="PGDDLDisplayConfiguration#assertTextEquals" locator-key1="SUCCESS_MESSAGE" />

		<execute action="PGDDLDisplay#selectFrameTop" />
	</command>

	<command name="tearDownCP">
		<execute macro="Page#cpGoto">
			<var name="portletName" value="Dynamic Data Lists" />
		</execute>

		<while>
			<condition action="CPDynamicdatalists#isElementPresent" locator-key1="LIST_TABLE_ACTIONS_1" />
			<then>
				<execute action="CPDynamicdatalists#click" locator-key1="LIST_TABLE_ACTIONS_1" />
				<execute action="CPDynamicdatalists#click" locator-key1="LIST_MENULIST_DELETE" />
				<execute action="CPDynamicdatalists#confirm" locator-key1="CONFIRMATION" value1="Are you sure you want to delete this?" />
				<execute action="CPDynamicdatalists#assertTextEquals" locator-key1="SUCCESS_MESSAGE" />
			</then>
		</while>
	</command>

	<command name="viewCP">
		<execute macro="Page#cpGoto">
			<var name="portletName" value="Dynamic Data Lists" />
		</execute>

		<execute action="CPDynamicdatalists#type" locator-key1="BASIC_SEARCH_FIELD" value1="${listName}" />
		<execute action="CPDynamicdatalists#click" locator-key1="BASIC_SEARCH_BUTTON" />
		<execute action="CPDynamicdatalists#assertTextEquals" locator-key1="LIST_TABLE_NAME_1" value1="${listName}" />
		<execute action="CPDynamicdatalists#assertTextEquals" locator-key1="LIST_TABLE_DESCRIPTION_1" value1="${listDescription}" />
		<execute action="CPDynamicdatalists#click" locator-key1="LIST_TABLE_NAME_1" value1="${listName}" />

		<execute action="CPDynamicdatalistsList#assertTextEquals" locator-key1="HEADER_TITLE" value1="${listName}" />
		<execute action="CPDynamicdatalistsList#assertElementPresent" locator-key1="ADD_RECORD_BUTTON" />
	</command>

	<command name="viewPG">
		<execute macro="Page#pgGoto">
			<var name="portletName" value="${pageName}" />
		</execute>

		<execute action="PGDDLDisplay#assertTextEquals" locator-key1="HEADER_TITLE" value1="${listName}" />
	</command>
</definition>