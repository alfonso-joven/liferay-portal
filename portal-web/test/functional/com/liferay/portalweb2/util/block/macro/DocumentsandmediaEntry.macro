<definition>
	<command name="addPG">
		<execute macro="Page#pgGoto">
			<var name="pageName" value="${pageName}" />
		</execute>

		<execute action="PGDocumentsandmedia#click" locator-key1="TOOLBAR_ADD" />
		<execute action="PGDocumentsandmedia#click" locator-key1="ADD_MENULIST_BASIC_DOCUMENT" />

		<execute action="PGDocumentsandmediaAdddocument#uploadCommonFile" locator-key1="FILE_FIELD" value1="Document_1.doc" />
		<execute action="PGDocumentsandmediaAdddocument#type" locator-key1="TITLE_FIELD" value1="${documentsandmediaEntryTitle}" />
		<execute action="PGDocumentsandmediaAdddocument#type" locator-key1="DESCRIPTION_FIELD" value1="${documentsandmediaEntryContent}" />
		<execute action="PGDocumentsandmediaAdddocument#click" locator-key1="PUBLISH_BUTTON" />

		<execute action="PGDocumentsandmedia#assertTextEquals" locator-key1="SUCCESS_MESSAGE" value1="Your request completed successfully." />
	</command>

	<command name="deletePG">
		<execute macro="Page#pgGoto">
			<var name="pageName" value="${pageName}" />
		</execute>

		<execute action="PGDocumentsandmedia#click" locator-key1="ENTRIES_DOCUMENT" />

		<execute action="PGDocumentsandmediaDocument#assertTextEquals" locator-key1="DOCUMENT_INFO_TITLE" value1="${documentsandmediaEntryTitle}" />
		<execute action="PGDocumentsandmediaDocument#click" locator-key1="TOOLBAR_DELETE_BUTTON" />
		<execute action="PGDocumentsandmedia#confirm" locator-key1="CONFIRMATION" value1="Are you sure you want to delete the selected entry?" />
		<execute action="PGDocumentsandmediaDocument#assertTextEquals" locator-key1="SUCCESS_MESSAGE" value1="Your request completed successfully." />
		<execute action="PGDocumentsandmediaDocument#assertElementNotPresent" locator-key1="DOCUMENT_INFO_TITLE" />
	</command>

	<command name="pgEdit">
		<execute macro="Page#pgGoto">
			<var name="pageName" value="${pageName}" />
		</execute>

		<execute action="PGDocumentsandmedia#click" locator-key1="ENTRIES_DOCUMENT" />

		<execute action="PGDocumentsandmediaDocument#assertTextEquals" locator-key1="DOCUMENT_INFO_TITLE" value1="${documentsandmediaEntryTitle}"/>
		<execute action="PGDocumentsandmediaDocument#click" locator-key1="TOOLBAR_EDIT_BUTTON" />
		<execute action="PGDocumentsandmediaEditdocument#type" locator-key1="TITLE_FIELD" value1="${documentsandmediaEntryTitleEdit}"  />
		<execute action="PGDocumentsandmediaEditdocument#type" locator-key1="DESCRIPTION_FIELD" value1="${documentsandmediaEntryContentEdit}" />
		<execute action="PGDocumentsandmediaEditdocument#click" locator-key1="PUBLISH_BUTTON" />

		<execute action="PGDocumentsandmediaDocument#assertTextEquals" locator-key1="SUCCESS_MESSAGE" value1="Your request completed successfully." />
		<execute action="PGDocumentsandmediaDocument#assertTextEquals" locator-key1="DOCUMENT_INFO_TITLE" value1="${documentsandmediaEntryTitleEdit}" />
	</command>

	<command name="moveToFolderPG">
			<execute macro="Page#pgGoto">
				<var name="pageName" value="${pageName}" />
			</execute>

			<execute action="PGDocumentsandmedia#click" locator-key1="ENTRIES_FOLDER" />

			<execute action="PGDocumentsandmediaFolder#check" locator-key1="FOLDER_ENTRIES_DOCUMENT_SELECT" />
			<execute action="PGDocumentsandmediaFolder#click" locator-key1="TOOLBAR_ACTIONS" />
			<execute action="PGDocumentsandmediaFolder#click" locator-key1="ACTIONS_MENULIST_MOVE" />

			<execute action="PGDocumentsandmediaMovefiles#click" locator-key1="SELECT" />
			<execute action="PGDocumentsandmediaMovefiles#selectWindow" locator-key1="SELECT_POP_UP" />
			<execute action="PGDocumentsandmediaMovefiles#click" locator-key1="SELECT_POP_UP_CHOOSE_2" />
			<execute action="PGDocumentsandmediaMovefiles#selectWindow" locator-key1="SELECT_NULL_WINDOW" />
			<execute action="PGDocumentsandmediaMovefiles#click" locator-key1="MOVE" />
			<execute action="PGDocumentsandmedia#assertTextEquals" locator-key1="SUCCESS_MESSAGE" value1="Your request completed successfully." />
	</command>


	<command name="tearDownCP">
		<execute macro="Page#cpGoto">
			<var name="portletName" value="Documents and Media" />
		</execute>

		<if>
			<condition action="PGDocumentsandmedia#isElementPresent" locator-key1="ENTRIES_DOCUMENT" />
			<then>
					<execute action="PGDocumentsandmedia#check" locator-key1="TOOLBAR_SELECT_ALL_CHECKBOX" />
					<execute action="PGDocumentsandmedia#click" locator-key1="TOOLBAR_ACTIONS" />
					<execute action="PGDocumentsandmedia#assertElementPresent" locator-key1="ACTIONS_MENULIST_DELETE" />
					<execute action="PGDocumentsandmedia#click" locator-key1="ACTIONS_MENULIST_DELETE" />
					<execute action="PGDocumentsandmedia#confirm" locator-key1="CONFIRMATION" value1="Are you sure you want to delete the selected entries?" />
					<execute action="PGDocumentsandmedia#assertTextEquals" locator-key1="SUCCESS_MESSAGE" value1="Your request completed successfully." />
					
			</then>
		</if>

	</command>

	<command name="viewCP">
		<execute macro="Page#pgGoto">
			<var name="pageName" value="${pageName}" />
		</execute>
		<execute action="PGDocumentsandmedia#type" locator-key1="TOOLBAR_SEARCH_FIELD" value1="${documentsandmediaEntryTitle}" />
		<execute action="PGDocumentsandmedia#click" locator-key1="TOOLBAR_SEARCH_BUTTON" />
		<execute action="PGDocumentsandmedia#assertTextEquals" locator-key1="ENTRIES_DOCUMENT_SEARCH_TITLE" value1="${documentsandmediaEntryTitle}" />
	</command>
</definition>