<definition>
	<command name="addCP">
		<execute macro="Page#cpGoto">
			<var name="portletName" value="Documents and Media" />
		</execute>

		<execute action="CPDocumentsandmedia#click" locator-key1="ADD" />
		<execute action="CPDocumentsandmedia#click" locator-key1="ADD_MENULIST_FOLDER" />

		<execute action="CPDocumentsandmediaAddfolder#type" locator-key1="NAME_FIELD" value1="${folderTitle}" />
		<execute action="CPDocumentsandmediaAddfolder#type" locator-key1="DESCRIPTION_FIELD" value1="${folderDescription}" />
		<execute action="CPDocumentsandmediaAddfolder#click" locator-key1="SAVE_BUTTON" />

		<execute action="CPDocumentsandmedia#assertTextEquals" locator-key1="REQUEST" value1="Your request completed successfully."/>

		<if>
			<condition action="CPDocumentsandmedia#isElementNotPresent" locator-key1="DOCUMENT_CONTAINER_FOLDER2" />
			<then>
				<execute action="CPDocumentsandmedia#assertElementPresent" locator-key1="DOCUMENT_CONTAINER_FOLDER1" />
				<execute action="CPDocumentsandmedia#assertTextEquals" locator-key1="DOCUMENT_CONTAINER_FOLDER1_TITLE" value1="${folderTitle}"/>
			</then>
			<else>
				<execute action="CPDocumentsandmedia#assertElementPresent" locator-key1="DOCUMENT_CONTAINER_FOLDER2" />
				<execute action="CPDocumentsandmedia#assertTextEquals" locator-key1="DOCUMENT_CONTAINER_FOLDER2_TITLE" value1="${folderTitle}"/>
			</else>
		</if>
	</command>

	<command name="addPGviaMG">
		<execute macro="Page#pgGoto">
			<var name="pageName" value="${pageName}" />
		</execute>

		<execute action="PGMediagallery#click" locator-key1="FOLDER_MENU_ADD_FOLDER" />

		<execute action="PGMediagalleryAddfolder#type" locator-key1="NAME_FIELD" value1="${folderName}" />
		<execute action="PGMediagalleryAddfolder#type" locator-key1="DESCRIPTION_FIELD" value1="${folderDescription}" />
		<execute action="PGMediagalleryAddfolder#click" locator-key1="SAVE_BUTTON" />
		<execute action="PGMediagalleryAddfolder#assertTextEquals" locator-key1="SUCCESS_MESSAGE" value1="Your request completed successfully." />

		<execute action="PGMediagallery#assertElementPresent" locator-key1="ENTRY_THUMBNAIL" />
		<execute action="PGMediagallery#assertTextEquals" locator-key1="ENTRY_TITLE" value1="${folderName}" />
	</command>

	<command name="deleteCP">
		<execute macro="Page#cpGoto">
			<var name="portletName" value="Documents and Media" />
		</execute>

		<execute action="CPDocumentsandmedia#assertElementPresent" locator-key1="DOCUMENT_CONTAINER_FOLDER1" />
		<execute action="CPDocumentsandmedia#assertTextEquals" locator-key1="DOCUMENT_CONTAINER_FOLDER1_TITLE" value1="${folderTitle}"/>

		<execute action="CPDocumentsandmedia#mouseOver" locator-key1="DOCUMENT_CONTAINER_FOLDER1" />
		<execute action="CPDocumentsandmedia#click" locator-key1="DOCUMENT_CONTAINER_FOLDER1_CHECKBOX" />
		<execute action="CPDocumentsandmedia#assertElementPresent" locator-key1="ACTIONS" />
		<execute action="CPDocumentsandmedia#click" locator-key1="ACTIONS" />
		<execute action="CPDocumentsandmedia#click" locator-key1="ACTIONS_MENULIST_DELETE" />
		<execute action="CPDocumentsandmedia#confirm" locator-key1="CONFIRMATION" value1="Are you sure you want to delete the selected entries?" />
		<execute action="CPDocumentsandmedia#assertTextEquals" locator-key1="REQUEST" value1="Your request completed successfully."/>

		<if>
			<condition action="CPDocumentsandmedia#isElementPresent" locator-key1="DOCUMENT_CONTAINER_FOLDER1" />
			<then>
				<execute action="CPDocumentsandmedia#assertTextNotEquals" locator-key1="DOCUMENT_CONTAINER_FOLDER1_TITLE" value1="${folderTitle}" />
			</then>
			<else>
				<execute action="CPDocumentsandmedia#assertElementNotPresent" locator-key1="DOCUMENT_CONTAINER_FOLDER1" />
			</else>
		</if>
	</command>

	<command name="editCP">
		<execute macro="Page#cpGoto">
			<var name="portletName" value="Documents and Media" />
		</execute>

		<execute action="CPDocumentsandmedia#assertElementPresent" locator-key1="DOCUMENT_CONTAINER_FOLDER1" />
		<execute action="CPDocumentsandmedia#assertTextEquals" locator-key1="DOCUMENT_CONTAINER_FOLDER1_TITLE" value1="${folderTitle}" />

		<execute action="CPDocumentsandmedia#mouseOver" locator-key1="DOCUMENT_CONTAINER_FOLDER1" />
		<execute action="CPDocumentsandmedia#assertElementPresent" locator-key1="DOCUMENT_CONTAINER_FOLDER1_DROPDOWN" />
		<execute action="CPDocumentsandmedia#click" locator-key1="DOCUMENT_CONTAINER_FOLDER1_DROPDOWN" />
		<execute action="CPDocumentsandmedia#assertElementPresent" locator-key1="FOLDER_MENULIST_EDIT" />
		<execute action="CPDocumentsandmedia#click" locator-key1="FOLDER_MENULIST_EDIT" />

		<execute action="CPDocumentsandmediaEditfolder#type" locator-key1="NAME_FIELD" value1="${folderTitleNew}" />
		<execute action="CPDocumentsandmediaEditfolder#type" locator-key1="DESCRIPTION_FIELD" value1="${folderDescriptionNew}" />
		<execute action="CPDocumentsandmediaEditfolder#click" locator-key1="SAVE_BUTTON" />

		<execute action="CPDocumentsandmedia#assertTextEquals" locator-key1="REQUEST" value1="Your request completed successfully."/>
		<execute action="CPDocumentsandmedia#assertElementPresent" locator-key1="DOCUMENT_CONTAINER_FOLDER1" />
		<execute action="CPDocumentsandmedia#assertTextEquals" locator-key1="DOCUMENT_CONTAINER_FOLDER1_TITLE" value1="${folderTitleNew}"/>
	</command>

	<command name="editPGviaMG">
		<execute macro="Page#pgGoto">
			<var name="pageName" value="${pageName}" />
		</execute>

		<execute action="PGMediagallery#assertTextEquals" locator-key1="ENTRY_TITLE" value1="${folderName}" />
		<execute action="PGMediagallery#click" locator-key1="ENTRY_TITLE" value1="${folderName}" />

		<execute action="PGMediagalleryFolder#click" locator-key1="FOLDER_MENU_EDIT" />

		<execute action="PGMediagalleryEditfolder#type" locator-key1="NAME_FIELD" value1="${folderName} Edit" />
		<execute action="PGMediagalleryEditfolder#type" locator-key1="DESCRIPTION_FIELD" value1="${folderDescription} Edit" />
		<execute action="PGMediagalleryEditfolder#click" locator-key1="SAVE_BUTTON" />
		<execute action="PGMediagalleryEditfolder#click" locator-key1="SUCCESS_MESSAGE" value1="Your request completed successfully." />
	</command>

	<command name="moveToFolderCP">
		<execute macro="Page#cpGoto">
			<var name="portletName" value="Documents and Media" />
		</execute>

		<execute action="CPDocumentsandmedia#assertElementPresent" locator-key1="DOCUMENT_CONTAINER_FOLDER1" />
		<execute action="CPDocumentsandmedia#assertElementPresent" locator-key1="DOCUMENT_CONTAINER_FOLDER2" />

		<execute action="CPDocumentsandmedia#click" locator-key1="DOCUMENT_CONTAINER_FOLDER1_DROPDOWN" />
		<execute action="CPDocumentsandmedia#assertElementPresent" locator-key1="FOLDER_MENULIST_MOVE" />
		<execute action="CPDocumentsandmedia#click" locator-key1="FOLDER_MENULIST_MOVE" />

		<execute action="CPDocumentsandmediaMovefolder#click" locator-key1="SELECT_PARENT_FOLDER_BUTTON" />
		<execute action="CPDocumentsandmediaMovefolder#selectWindow" locator-key1="FOLDER_WINDOW_ID" />
		<execute action="CPDocumentsandmediaMovefolder#click" locator-key1="FOLDER_WINDOW_FOLDER2_CHOOSE_BUTTON" />
		<execute action="CPDocumentsandmediaMovefolder#selectWindow" locator1="null" />

		<execute action="CPDocumentsandmediaMovefolder#assertTextEquals" locator-key1="PARENT_FOLDER" value1="${folderTitle2}" />
		<execute action="CPDocumentsandmediaMovefolder#click" locator-key1="MOVE_BUTTON" />

		<execute action="CPDocumentsandmedia#assertTextEquals" locator-key1="REQUEST" value1="Your request completed successfully."/>
		<execute action="CPDocumentsandmedia#assertElementNotPresent" locator-key1="DOCUMENT_CONTAINER_FOLDER2" />
		<execute action="CPDocumentsandmedia#click" locator-key1="DOCUMENT_CONTAINER_FOLDER1" />
		<execute action="CPDocumentsandmedia#assertElementPresent" locator-key1="FOLDER_VIEW_FOLDER1" />
	</command>

	<command name="tearDownPGviaMG" >
		<execute macro="Page#pgGoto">
			<var name="pageName" value="${pageName}" />
		</execute>

		<while>
			<condition action="PGMediagallery#isElementPresent" locator-key1="ENTRY_THUMBNAIL" />
			<then>
				<execute action="PGMediagallery#click" locator-key1="ENTRY_TITLE" />

				<execute action="PGMediagalleryFolder#click" locator-key1="FOLDER_MENU_DELETE" />
				<execute action="PGMediagalleryFolder#confirm" locator-key1="CONFIRMATION" value1="Are you sure you want to delete this?" />
				<execute action="PGMediagalleryFolder#assertTextEquals" locator-key1="SUCCESS_MESSAGE" value1="Your request completed successfully." />
			</then>
		</while>
	</command>

	<command name="tearDownCP">
		<execute macro="Page#cpGoto">
			<var name="portletName" value="Documents and Media" />
		</execute>

		<while>
			<condition action="CPDocumentsandmedia#isElementPresent" locator-key1="DOCUMENT_CONTAINER_FOLDER1" />
			<then>
				<execute action="CPDocumentsandmedia#select" locator-key1="PAGINATION_ROWS_PER_PAGE_SELECT" value1="75" />
				<execute action="CPDocumentsandmedia#check" locator-key1="SELECT_ALL_ROWS_CHECKBOX" />
				<execute action="CPDocumentsandmedia#click" locator-key1="ACTIONS" />
				<execute action="CPDocumentsandmedia#assertElementPresent" locator-key1="ACTIONS_MENULIST_DELETE" />
				<execute action="CPDocumentsandmedia#click" locator-key1="ACTIONS_MENULIST_DELETE" />
				<execute action="CPDocumentsandmedia#confirm" locator-key1="CONFIRMATION" value1="Are you sure you want to delete the selected entries?" />
				<execute action="CPDocumentsandmedia#assertTextEquals" locator-key1="REQUEST" value1="Your request completed successfully."/>
			</then>
		</while>
	</command>
</definition>