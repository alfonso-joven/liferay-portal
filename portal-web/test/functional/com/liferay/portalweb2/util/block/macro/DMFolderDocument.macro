<definition>
	<command name="addPGviaDMD">
		<execute macro="Page#pgGoto">
			<var name="pageName" value="${pageName}" />
		</execute>

		<execute action="PGDocumentlibrarydisplay#click" locator-key1="FOLDERS_TABLE_NAME_1" value1="${documentFolderName}" />

		<execute action="PGDocumentlibrarydisplayFolder#click" locator-key1="SUBFOLDER_MENU_ADD_DOCUMENT" value1="${documentName}" />

		<execute action="PGDocumentlibrarydisplayAdddocument#selectFrame" locator-key1="SELECT_DOCUMENT_TYPE_IFRAME" />
		<execute action="PGDocumentlibrarydisplayAdddocument#click" locator-key1="SELECT_DOCUMENT_TYPE_BASIC_DOCUMENT" />
		<execute action="PGDocumentlibrarydisplayAdddocument#selectFrame" />
		<execute action="PGDocumentlibrarydisplayAdddocument#uploadCommonFile" locator-key1="DOCUMENT_FILE_FIELD" value1="${documentFileName}" />
		<execute action="PGDocumentlibrarydisplayAdddocument#type" locator-key1="DOCUMENT_TITLE_FIELD" value1="${documentName}" />
		<execute action="PGDocumentlibrarydisplayAdddocument#click" locator-key1="PUBLISH_BUTTON" />

		<execute action="PGDocumentlibrarydisplayFolder#assertTextEquals" locator-key1="SUCCESS_MESSAGE" />
		<execute action="PGDocumentlibrarydisplayFolder#assertTextEquals" locator-key1="DOCUMENTS_TABLE_NAME_1" value1="${documentName}" />
		<execute action="PGDocumentlibrarydisplayFolder#click" locator-key1="DOCUMENTS_TABLE_NAME_1" value1="${documentName}" />

		<execute action="PGDocumentlibrarydisplayDocument#assertTextEquals" locator-key1="DOCUMENT_HEADER_TITLE" value1="${documentName}" />
		<execute action="PGDocumentlibrarydisplayDocument#assertTextEquals" locator-key1="DOCUMENT_TITLE" value1="${documentName}" />
		<execute action="PGDocumentlibrarydisplayDocument#assertTextEquals" locator-key1="DOCUMENT_VERSION" value1="Version 1.0" />
	</command>

	<command name="addPGviaMG">
		<execute macro="Page#pgGoto">
			<var name="pageName" value="${pageName}" />
		</execute>

		<execute action="PGMediagallery#assertTextEquals" locator-key1="ENTRY_TITLE" value1="${folderName}" />
		<execute action="PGMediagallery#click" locator-key1="ENTRY_TITLE" value1="${folderName}" />

		<execute action="PGMediagalleryFolder#assertTextEquals" locator-key1="INFO_MESSAGE" value1="There are no media files in this folder." />
		<execute action="PGMediagalleryFolder#click" locator-key1="FOLDER_MENU_ADD_MEDIA" />

		<execute action="PGMediagallerySelectmediatype#selectFrame" locator-key1="SELECT_MEDIA_TYPE_IFRAME" />
		<execute action="PGMediagallerySelectmediatype#click" locator-key1="MEDIA_TABLE_BASIC_DOCUMENT" />
		<execute action="PGMediagallerySelectmediatype#selectFrame" />

		<execute action="PGMediagalleryAddmedia#uploadCommonFile" locator-key1="FILE" value1="${mediaFile}" />
		<execute action="PGMediagalleryAddmedia#type" locator-key1="TITLE_FIELD" value1="${mediaTitle}" />
		<execute action="PGMediagalleryAddmedia#type" locator-key1="DESCRIPTION_FIELD" value1="${mediaDescription}" />
		<execute action="PGMediagalleryAddmedia#click" locator-key1="PUBLISH_BUTTON" />
		<execute action="PGMediagalleryAddmedia#assertTextEquals" locator-key1="SUCCESS_MESSAGE" value1="Your request completed successfully." />

		<execute action="PGMediagallery#assertTextEquals" locator-key1="ENTRY_TITLE" value1="${folderName}" />
		<execute action="PGMediagallery#click" locator-key1="ENTRY_TITLE" value1="${folderName}" />

		<execute action="PGMediagalleryFolder#assertElementPresent" locator-key1="MEDIA_THUMBNAIL" />
		<execute action="PGMediagalleryFolder#assertElementPresent" locator-key1="MEDIA_TITLE" value1="${mediaTitle}" />
	</command>

	<command name="deletePGviaMG">
		<execute macro="Page#pgGoto">
			<var name="pageName" value="${pageName}" />
		</execute>

		<execute action="PGMediagallery#assertTextEquals" locator-key1="ENTRY_TITLE" value1="${folderName}" />
		<execute action="PGMediagallery#click" locator-key1="ENTRY_TITLE" value1="${folderName}" />

		<execute action="PGMediagalleryFolder#click" locator-key1="MEDIA_TITLE" value1="${mediaTitle}"/>

		<execute action="PGMediagalleryMedia#click" locator-key1="DELETE" />
		<execute action="PGMediagalleryMedia#confirm" locator-key1="CONFIRMATION" value1="Are you sure you want to delete this?" />

		<execute action="PGMediagalleryFolder#assertTextEquals" locator-key1="SUCCESS_MESSAGE" value1="Your request completed successfully." />
		<execute action="PGMediagalleryFolder#assertTextEquals" locator-key1="INFO_MESSAGE" value1="There are no media files in this folder." />
	</command>

	<command name="editPGviaMG">
		<execute macro="Page#pgGoto">
			<var name="pageName" value="${pageName}" />
		</execute>

		<execute action="PGMediagallery#assertTextEquals" locator-key1="ENTRY_TITLE" value1="${folderName}" />
		<execute action="PGMediagallery#click" locator-key1="ENTRY_TITLE" value1="${folderName}" />

		<execute action="PGMediagalleryFolder#click" locator-key1="MEDIA_TITLE" value1="${mediaTitle}"/>

		<execute action="PGMediagalleryMedia#assertTextEquals" locator-key1="MEDIA_CAPTION" value1="${mediaTitle} - ${mediaDescription}" />
		<execute action="PGMediagalleryMedia#click" locator-key1="EDIT" />

		<execute action="PGMediagalleryEditmedia#type" locator-key1="TITLE_FIELD" value1="${mediaTitle} Edit" />
		<execute action="PGMediagalleryEditmedia#type" locator-key1="DESCRIPTION_FIELD" value1="${mediaDescription} Edit" />
		<execute action="PGMediagalleryEditmedia#click" locator-key1="PUBLISH_BUTTON" />
		<execute action="PGMediagalleryEditmedia#assertTextEquals" locator-key1="SUCCESS_MESSAGE" value1="Your request completed successfully." />

		<execute action="PGMediagalleryFolder#assertTextEquals" locator-key1="MEDIA_TITLE" value1="${mediaTitle} Edit" />
	</command>

	<command name="searchPGviaDMD">
		<execute macro="Page#pgGoto">
			<var name="pageName" value="${pageName}" />
		</execute>

		<execute action="PGDocumentlibrarydisplay#type" locator-key1="SEARCH_FIELD" value1="${documentNameSearch}" />
		<execute action="PGDocumentlibrarydisplay#click" locator-key1="SEARCH_BUTTON" />

		<execute action="PGDocumentlibrarydisplaySearch#assertTextEquals" locator-key1="SEARCH_RESULTS_FOLDER" value1="${documentFolderName}" />
		<execute action="PGDocumentlibrarydisplaySearch#assertTextEquals" locator-key1="SEARCH_RESULTS_DOCUMENT" value1="${documentName}" />
	</command>

	<command name="searchNoPGviaDMD">
		<execute macro="Page#pgGoto">
			<var name="pageName" value="${pageName}" />
		</execute>

		<execute action="PGDocumentlibrarydisplaySearch#type" locator-key1="SEARCH_FIELD" value1="${documentNameSearch}" />
		<execute action="PGDocumentlibrarydisplaySearch#click" locator-key1="SEARCH_BUTTON" />
		<execute action="PGDocumentlibrarydisplaySearch#assertTextEquals" locator-key1="INFO_MESSAGE" value1="No documents were found that matched the keywords: ${documentNameSearch}." />
	</command>
</definition>