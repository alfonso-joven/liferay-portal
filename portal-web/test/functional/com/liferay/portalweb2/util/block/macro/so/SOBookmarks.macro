<definition>
	<command name="addBookmarksEntry">
		<execute action="PGSOHome#click" locator-key1="BOOKMARKS_ADD_ENTRY_LINK" />
		
		<execute action="PGSOHomeBookmarksAdd#type" locator-key1="NAME_FIELD" value1="${bookmarksEntryName}" />
		<execute action="PGSOHomeBookmarksAdd#type" locator-key1="URL_FIELD" value1="${bookmarksEntryUrl}" />
		<execute action="PGSOHomeBookmarksAdd#type" locator-key1="DESCRIPTION_FIELD" value1="${bookmarksEntryDescription}" />
		<execute action="PGSOHomeBookmarksAdd#click" locator-key1="SAVE_BUTTON"  />

		<execute action="PGSOHome#assertTextEquals" locator-key1="SUCCESS_MESSAGE" value1="Your request completed successfully." />
		<execute action="PGSOHome#assertTextEquals" locator-key1="BOOKMARKS_ENTRY_LINK" value1="${bookmarksEntryName}" />
	</command>

	<command name="editBookmarksEntry">
		<execute action="PGSOHome#assertClick" locator-key1="BOOKMARKS_ENTRY_ACTIONS" value1="Actions" />
		<execute action="PGSOHome#assertClick" locator-key1="ACTIONS_MENULIST_EDIT" value1="Edit" />
		
		<execute action="PGSOHomeBookmarksAdd#type" locator-key1="NAME_FIELD" value1="${bookmarksEntryNameEdit}" />
		<execute action="PGSOHomeBookmarksAdd#type" locator-key1="URL_FIELD" value1="${bookmarksEntryUrlEdit}" />
		<execute action="PGSOHomeBookmarksAdd#type" locator-key1="DESCRIPTION_FIELD" value1="${bookmarksEntryDescriptionEdit}" />
		<execute action="PGSOHomeBookmarksAdd#click" locator-key1="SAVE_BUTTON"  />

		<execute action="PGSOHome#assertTextEquals" locator-key1="SUCCESS_MESSAGE" value1="Your request completed successfully." />
		<execute action="PGSOHome#assertTextEquals" locator-key1="BOOKMARKS_ENTRY_LINK" value1="${bookmarksEntryNameEdit}" />
	</command>

	<command name="deleteBookmarksEntry">
		<execute action="PGSOHome#assertClick" locator-key1="BOOKMARKS_ENTRY_ACTIONS" value1="Actions" />
		<execute action="PGSOHome#assertClick" locator-key1="ACTIONS_MENULIST_DELETE" value1="Delete" />
		<execute action="PGSOHome#confirm" locator-key1="CONFIRMATION" value1="Are you sure you want to delete this?" />
		<execute action="PGSOHome#assertTextEquals" locator-key1="SUCCESS_MESSAGE" value1="Your request completed successfully." />
		<execute action="PGSOHome#assertTextEquals" locator-key1="BOOKMARKS_NO_ENTRIES_MESSAGE" value1="There are no entries." />
		<execute action="PGSOHome#assertElementNotPresent" locator-key1="BOOKMARKS_ENTRY_LINK" />
	</command>

	<command name="viewBookmarksEntry">
		<execute action="PGSOHome#click" locator-key1="BOOKMARKS_ENTRY_LINK" />

		<if>
			<equals arg1="${bookmarksEntryUrl}" arg2="http://www.google.com" />
			<then>
				<execute action="PGSOHome#selectWindow" locator-key1="BOOKMARKS_POP_UP_GOOGLE" />
				<execute action="PGSOHome#assertElementPresent" locator-key1="BOOKMARKS_POP_UP_IMAGE_GOOGLE" />
				<execute action="PGSOHome#close" locator-key1="BOOKMARKS_POP_UP_GOOGLE" />
			</then>
			<elseif>
				<equals arg1="${bookmarksEntryUrl}" arg2="http://www.yahoo.com" />
				<then>
					<execute action="PGSOHome#selectWindow" locator-key1="BOOKMARKS_POP_UP_YAHOO" />
					<execute action="PGSOHome#assertElementPresent" locator-key1="BOOKMARKS_POP_UP_IMAGE_YAHOO" />
					<execute action="PGSOHome#close" locator-key1="BOOKMARKS_POP_UP_YAHOO" />
				</then>
			</elseif>
			<else>
				<execute action="PGSOHome#selectWindow" locator-key1="BOOKMARKS_POP_UP_MSN" />
				<execute action="PGSOHome#assertElementPresent" locator-key1="BOOKMARKS_POP_UP_IMAGE_MSN" />
				<execute action="PGSOHome#close" locator-key1="BOOKMARKS_POP_UP_MSN" />
			</else>
		</if>

		<execute action="PGSOHome#assertClick" locator-key1="BOOKMARKS_ENTRY_ACTIONS" value1="Actions" />
		<execute action="PGSOHome#assertClick" locator-key1="ACTIONS_MENULIST_EDIT" value1="Edit" />

		<execute action="PGSOHomeBookmarksAdd#assertTextEquals" locator-key1="NAME_FIELD" value1="${bookmarksEntryName}" />
		<execute action="PGSOHomeBookmarksAdd#assertTextEquals" locator-key1="URL_FIELD" value1="${bookmarksEntryUrl}" />
		<execute action="PGSOHomeBookmarksAdd#assertTextEquals" locator-key1="DESCRIPTION_FIELD" value1="${bookmarksEntryDescription}" />
	</command>
</definition>