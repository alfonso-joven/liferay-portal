		</execute>

		<execute macro="Category#deleteSubcategoryActionsCP">
			<var name="subcategoryName" value="${subcategoryName}" />
		</execute>
	</command>

	<tear-down>
		<execute macro="Category#tearDownVocabularyCP" />
	</tear-down>
</definition>