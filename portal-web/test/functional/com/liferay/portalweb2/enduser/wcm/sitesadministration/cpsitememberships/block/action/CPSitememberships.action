<definition>
	<command name="assertClick">
		<case locator-key1="ADD_SITE_ROLES_TO_MENULIST_USERS">
			<execute function="AssertClick#assertTextClickAtAndWait" />
		</case>
		<case locator-key1="USER_SEARCH_BUTTON">
			<execute function="AssertClick#assertValueClickAtAndWait" />
		</case>		
		<case locator-key1="TOOLBAR_ADD_SITE_ROLES_TO">
			<execute function="AssertClick#assertTextClickAt" />
		</case>
		<case locator-key1="TOOLBAR_VIEW_TEAMS">
			<execute function="AssertClick#assertTextClickAtAndWait" />
		</case>
	</command>

	<command name="assertTextEquals">
		<case locator-key1="SUCCESS_MESSAGE">
			<execute function="AssertTextEquals#assertText" value1="Your request completed successfully." />
		</case>
	</command>
</definition>