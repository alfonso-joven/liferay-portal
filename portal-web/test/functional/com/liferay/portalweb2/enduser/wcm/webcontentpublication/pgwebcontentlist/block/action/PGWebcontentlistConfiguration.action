<definition>
	<command name="click">
		<case locator-key1="SAVE_BUTTON">
			<execute function="Click#valueClickAtAndWait" value1="Save" />
		</case>
	</command>

	<command name="selectFrame">
		<case locator-key1="CONFIGURATION_IFRAME">
			<execute function="SelectFrame#selectFrameWaitForScript" />
		</case>
	</command>
</definition>
