<definition>
	<command name="assertClick">
		<case locator-key1="ROLES_SEARCH_BUTTON">
			<execute function="AssertClick#assertValueClickAtAndWait" />
		</case>
	</command>
	
	<command name="click">
		<case comparator="startsWith" locator-key1="ROLE_TABLE_TITLE_1">
			<execute function="Click#clickAtAndWait" />
		</case>
	</command>
</definition>