<definition>
	<command name="assertTextEquals">
		<case locator-key1="INFO_MESSAGE">
			<execute function="AssertTextEquals#assertText" value1="There are no tags." />
		</case>
		<case locator-key1="SUCCESS_MESSAGE">
			<execute function="AssertTextEquals#assertText" value1="Your request processed successfully." />
		</case>
	</command>

	<command name="click">
		<case locator-key1="ADD_TAG_SAVE">
			<execute function="Click#valueClickAt" value1="Save" />
		</case>
	</command>
</definition>