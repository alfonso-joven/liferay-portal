<definition>
	<command name="assertTextEquals">
		<case locator-key1="SUCCESS_MESSAGE">
			<execute function="AssertTextEquals#assertText" value1="Your request completed successfully." />
		</case>
	</command>

	<command name="assertClick">
		<case locator-key1="ADD_BUTTON">
			<execute function="AssertClick#assertTextClickAtAndWait" />
		</case>
		<case comparator="startsWith" locator-key1="PAGE_TEMPLATE_TABLE_ACTIONS_">
			<execute function="AssertClick#assertTextClickAt" value1="Actions" />
		</case>
		<case locator-key1="PAGE_TEMPLATE_MENULIST_EDIT">
			<execute function="AssertClick#assertTextClickAtAndWait" />
		</case>
		<case locator-key1="PAGE_TEMPLATE_MENULIST_DELETE">
			<execute function="AssertClick#assertTextClickAtAndWait" />
		</case>
	</command>
</definition>