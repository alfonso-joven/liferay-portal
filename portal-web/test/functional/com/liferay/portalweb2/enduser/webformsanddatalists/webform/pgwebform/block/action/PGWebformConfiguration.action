<definition>
	<command name="assertTextEquals">
		<case locator-key1="SUCCESS_MESSAGE">
			<execute function="AssertTextEquals#assertText" value1="You have successfully updated the setup." />
		</case>
	</command>

	<command name="click">
		<case locator-key1="SAVE_BUTTON">
			<execute function="Click#valueClickAtAndWait" value1="Save" />
		</case>
	</command>

	<command name="type">
		<case locator-key1="SETUP_FORM_INFORMATION_TITLE">
			<execute function="Type#clickAtSendKeys" />
		</case>
		<case locator-key1="SETUP_FORM_INFORMATION_DESCRIPTION">
			<execute function="Type#clickAtSendKeys" />
		</case>
		<case locator-key1="SETUP_FORM_FIELDS_NAME">
			<execute function="Type#clickAtSendKeys" />
		</case>
		<case locator-key1="SETUP_FORM_FIELDS_RATING">
			<execute function="Type#clickAtSendKeys" />
		</case>
		<case locator-key1="SETUP_FORM_FIELDS_COMMENTS">
			<execute function="Type#clickAtSendKeys" />
		</case>
	</command>
</definition>