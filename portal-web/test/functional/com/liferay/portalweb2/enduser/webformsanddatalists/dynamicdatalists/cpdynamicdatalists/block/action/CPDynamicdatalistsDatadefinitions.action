<definition>
	<command name="assertTextEquals">
		<case locator-key1="SUCCESS_MESSAGE">
			<execute function="AssertTextEquals#assertText" value1="Your request completed successfully." />
		</case>
	</command>

	<command name="click">
		<case locator-key1="BASIC_SEARCH_BUTTON">
			<execute function="Click#valueClickAtAndWait" value1="Search" />
		</case>
		<case locator-key1="DATA_DEFINITION_MENULIST_DELETE">
			<execute function="Click#textClickAtAndWait" value1="Delete" />
		</case>
		<case locator-key1="DATA_DEFINITION_MENULIST_EDIT">
			<execute function="Click#textClickAtAndWait" value1="Edit" />
		</case>
		<case comparator="startsWith" locator-key1="DATA_DEFINITION_TABLE_ACTIONS_">
			<execute function="Click#textClickAt" value1="Actions" />
		</case>
		<case locator-key1="TOOLBAR_ADD">
			<execute function="Click#textClickAtAndWait" value1="Add" />
		</case>
	</command>
</definition>