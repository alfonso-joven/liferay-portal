<definition>
	<command name="assertClick">
		<case locator-key1="NAVIGATION_MY_POSTS">
			<execute function="AssertClick#assertTextClickAtAndWait" />
		</case>
		<case locator-key1="POST_NEW_THREAD_BUTTON">
			<execute function="AssertClick#assertValueClickAtAndWait" />
		</case>
		<case locator-key1="THREAD_TABLE_THREAD">
			<execute function="AssertClick#assertTextClickAtAndWait" />
		</case>
	</command>

	<command name="assertTextEquals">
		<case comparator="startsWith" locator-key1="THREAD_TABLE_LAST_POST">
			<execute function="AssertTextEquals#assertPartialText" />
		</case>
	</command>

	<command name="click">
		<case locator-key1="ADD_CATEGORY_BUTTON">
			<execute function="Click#valueClickAtAndWait" value1="Add Category" />
		</case>
		<case locator-key1="CATEGORY_MENULIST_DELETE">
			<execute function="Click#textClickAtAndWait" value1="Delete" />
		</case>
		<case comparator="startsWith" locator-key1="CATEGORY_TABLE_ACTIONS">
			<execute function="Click#pauseTextClickAt" value1="Actions" />
		</case>
		<case comparator="startsWith" locator-key1="CATEGORY_TABLE_CATEGORY">
			<execute function="Click#textClickAtAndWait" />
		</case>
		<case locator-key1="THREAD_MENULIST_DELETE">
			<execute function="Click#textClickAtAndWait" value1="Delete" />
		</case>
		<case comparator="startsWith" locator-key1="THREAD_TABLE_ACTIONS">
			<execute function="Click#pauseTextClickAt" value1="Actions" />
		</case>
	</command>
</definition>