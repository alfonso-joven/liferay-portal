<definition>
	<var name="pageName" value="Calendar Page" />
	<var name="portletName" value="Calendar" />

	<set-up>
		<execute macro="User#pgFirstLogin" />

		<execute macro="Page#pgAdd" />

		<execute macro="Portlet#pgAdd" />
	</set-up>

	<command name="AddCalendarEvent">
		<var name="eventTitle" value="Calendar Event Title" />

		<execute macro="Event#pgAdd" />
	</command>

	<command name="DeleteCalendarEvent">
		<var name="eventTitle" value="Calendar Event Title" />

		<execute macro="Event#pgAdd" />

		<execute macro="Event#pgDelete" />
	</command>

	<command name="EditCalendarEvent">
		<var name="eventTitle" value="Calendar Event Title" />
		<var name="eventTitleEdit" value="Calendar Event Title Edit" />

		<execute macro="Event#pgAdd" />

		<execute macro="Event#pgEdit" />
	</command>

	<tear-down>
		<execute macro="Event#cpTearDown" />

		<execute macro="Page#pgTearDown" />

		<execute macro="User#pgLogout" />
	</tear-down>
</definition>